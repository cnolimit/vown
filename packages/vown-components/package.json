{
  "name": "@vown/components",
  "version": "1.1.1",
  "description": "veriown components library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "veriown",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Veriown/vown.git",
    "directory": "packages/vown-components"
  },
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf ./dist",
    "compile": "tsc",
    "test": "jest",
    "version-bump": "version-bump @vown/components"
  },
  "dependencies": {
    "@material-ui/core": "4.5.1",
    "@material-ui/styles": "4.5.0",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "styled-components": "4.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "4.1.0",
    "@testing-library/react": "9.1.4",
    "@types/styled-components": "4.1.19",
    "@types/testing-library__react": "9.1.1",
    "@vown/scripts": "1.1.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/dist"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
